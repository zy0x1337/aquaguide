    "framer-motion": "^11.0.3",
    "fuse.js": "^7.0.0",
    "lucide-react": "^0.309.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.21.3",
    "recharts": "^2.12.7",
    "tailwind-merge": "^2.6.0"
